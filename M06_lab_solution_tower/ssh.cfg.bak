
Host bastion
  Hostname bastion.7b21.example.opentlc.com
  User ankay-redhat.com

Host *.internal 
  ProxyJump bastion
  User ec2-user

Host *.example.opentlc.com
  User ec2-user

Match User ec2-user
  IdentityFile ~/.ssh/7b21key.pem

Match User ankay-redhat.com
  IdentityFile ~/.ssh/id_rsa

Host *
  StrictHostKeyChecking no
  ForwardAgent yes
  ControlMaster auto
  ControlPath /tmp/%h-%r
  ControlPersist 5m
